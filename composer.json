{
    "name": "rvanmarkus/modx-laravel-models",
    "description": "Build your application / website with help of Laravel and MODX",
    "keywords": [
        "MODX",
        "Eloquent",
        "laravel",
        "Models",
        "Intergration",
        "PDO",
        "Database",
        "CMS"
    ],
    "homepage": "https://github.com/rvanmarkus/modx-laravel-models",
    "license": "MIT",
    "authors": [
        {
            "name": "Robbert van Markus",
            "email": "rvanmarkus@xebia.com",
            "homepage": "http://xebia.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.0",
        "php" : ">=5.3.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rvanmarkus\\Modxmodels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rvanmarkus\\Modxmodels\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
